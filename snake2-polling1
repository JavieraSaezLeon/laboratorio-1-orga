.text
main:

jal leerMatriz
j Fin

leerMatriz:
la $a2,mapa
add $t0,$a2,$zero
lw $t1,0($t0)
add $t3,$zero,$zero #filas
j forFilas
forFilas:
beq $t3,16,finforFilas
add $t4, $zero,$zero
j forColumnas
forColumnas:
beq $t4,32,finforColumnas
beq $t1,0,ppiso
beq $t1,1,ppared
beq $t1,2,pcabeza
beq $t1,3,pcuerpo

sumaForC:
add $t0,$t0,4
lw $t1, 0($t0)
add $t4,$t4,1
j forColumnas
finforColumnas:
add $t3,$t3,1
j forFilas
finforFilas:
jal polling
jr $ra

ppiso:
la $a0, piso
la $a1, display
add $s0, $t4, $zero
add $s1, $t3, $zero
jal pintar
j sumaForC

ppared:
la $a0, pared
la $a1, display
add $s0, $t4, $zero
add $s1, $t3, $zero
jal pintar
j sumaForC

pcabeza:
#la $a3,serpiente
#sw $t0,0($a3)
la $a0, cabeza
la $a1, display
add $s0, $t4, $zero
add $s1, $t3, $zero
jal pintar
j sumaForC

pcuerpo:
la $a0, cuerpo
la $a1, display
add $s0, $t4, $zero
add $s1, $t3, $zero
jal pintar
j sumaForC


#la $a0, cabeza # la direccion del rombo
#la $a1, display     # la direcion del display
   
#jal MOSTRAR         # llamamos a mostrar


# $a0 direccion del array
# $a1 direcion de inicio del display
pintar:
add $t6, $a0, $zero		# t6 direccion del array
add $t7, $a1, $zero		# t7 direccion del display
addi $t5, $zero, 0		# columna actual
mul $s0,$s0,64 #64
mul $s1,$s1,16 #16
j pintarMatriz
pintarMatriz:
add $a1,$a1,$s0
add $t7,$a1,$zero		
add $t5, $zero, $s1	
mul $t1, $t5, 2048                
add $t7, $a1, $t1 
WHILE_COLUMNA:                     # mientras mostremos una columna
lw $t1, 0($t6)                     # cargamos la palabra de la direccion dem meoria del array
beq $t1, 0x01000000, SALTAR_FILA            # si nos encontramos con un cero hay que saltar a la siguiente fila
sw $t1, 0($t7)                     # si no nos saltamos la fila, entonces copiamos el valor al display
addi $t6, $t6, 4                   # nos movemos una palabra en el array
addi $t7, $t7, 4                   # nos movemos una palabra en el display
j WHILE_COLUMNA

SALTAR_FILA:                        # Nos vamos a saltar una fila, pero debemos revisar si no hemos terminado (dos ceros seguidos)
addi $t6, $t6, 4                    # Nos movemos al siguiente elemento del array
lw $t1, 0($t6)                      # cargamos el siguiente elemento del array
beq $t1, 0x01000000, FIN_MOSTRAR             # ¿Hay un cero?, entonces encontramos dos ceros seguidos y terminamos

addi $t5, $t5, 1                    # ¿no encontramos un cero?, entonces nos movemos a la siguiente columa

mul $t1, $t5, 2048                  # la direccion de la siguiente columna es: NUMERO_COLUMNA * 2048), 2048 ya que es un array de words: 4*512

add $t7, $a1, $t1                   # la nueva direccion del display es: NUMERO_COLUMNA * 2048 + Display => offset + display
j WHILE_COLUMNA                     # mostramos la siguiente columa

FIN_MOSTRAR:                        # Encontramos los dos ceros seguidos asi que terminamos
jr $ra                              # retornamos

# Polling del teclado. Vamos preguntando cada 100ms si el estado del teclado es
# 'A' u otro valor
polling:
addi $a0, $zero, 100	# cada 100 ms
addi $v0, $zero, 32     # esperamos (syscall 32 sleep)
syscall			        # esperamos 100ms para no utilizar todo el procesador

addi $t9, $zero, 0xffff0004		# los caracteres del teclado se guardan en la dir de mem 0xffff0004
lw $t8, 0($t9)
sw $zero, 0($t9)		        # Seteamos a NULL (cero) el buffer para no leer datos duplicados

#beq $t1, 97, moverDerecha   # a mayuscula es 97 en ascii
#beq $t1, 100, moverIzquierda # d mayuscula es 100 en ascii
beq $t8, 119, moverArriba    # w mayuscula es 119 en ascii
#beq $t1,115, moverAbajo    # s mayuscula es 115 en ascii



moverArriba:
add $s1,$zero,0
add $s2,$zero,2
la $a2,mapa
#la $a3,serpiente
add $t0,$a2,$zero
#j mover
lw $t1,0($t0) #posicion display
add $t3,$zero,$zero #filas
j forFilasM
forFilasM:
beq $t3,16,finforFilasM
add $t4, $zero,$zero #columnas
j forColumnasM
forColumnasM:
beq $t4,32,finforColumnasM
beq $t1,2,mover
j sumaForCM
sumaForCM:
add $t0,$t0,4
lw $t1, 0($t0)
add $t4,$t4,1
j forColumnasM
finforColumnasM:
add $t3,$t3,1
j forFilasM
finforFilasM:
jr $ra

mover:

sw $s1,0($t0)
sub $t0,$t0,128
sw $s2,0($t0)

j leerMatriz



Fin:
